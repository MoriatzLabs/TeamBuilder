# GRID API
GRID_API_KEY=your_grid_api_key_here
GRID_GRAPHQL_ENDPOINT=https://api.grid.gg/central-data/graphql
GRID_SERIES_STATE_ENDPOINT=https://api.grid.gg/live-data-feed/graphql
GRID_STATS_FEED_ENDPOINT=https://api.grid.gg/stats-feed/graphql
GRID_WS_ENDPOINT=wss://api.grid.gg/live-data-feed

# Application
PORT=3000
NODE_ENV=development

# Redis
REDIS_HOST=localhost
REDIS_PORT=6379

# Cerebras API (optional - falls back to mock recommendations)
# Get your API key at https://cloud.cerebras.ai/
CEREBRAS_API_KEY=your_cerebras_api_key_here
CEREBRAS_MODEL=llama-4-scout-17b-16e-instruct

# Database
DATABASE_URL=postgresql://teambuilder:teambuilder@localhost:5432/teambuilder
